#!/usr/bin/env bash

# navigation
alias ..="pushd .."
alias ...="pushd ../.."
alias ....="pushd ../../.."
alias .....="pushd ../../../.."
alias ......="pushd ../../../../.."
alias .......="pushd ../../../../../.."
alias ........="pushd ../../../../../../.."
alias ~="cd ~"

# listing files
alias ll="ls -halF"
alias la="ls -A"
alias l="ls -CF"

# python
alias py="python3"

# clipboard
alias clip="xclip -select clip"

# disk usage
alias dud="du -h -d 1"

alias qual="cd ~/programming/qualified/qualified"
